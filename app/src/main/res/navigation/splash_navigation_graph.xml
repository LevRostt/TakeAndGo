<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_navigation_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="edu.mirea.levrost.takeandgo.takeandgo.ui.view.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" />

    <activity
        android:id="@+id/mainActivity"
        android:name="edu.mirea.levrost.takeandgo.takeandgo.ui.view.activity.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <argument
            android:name="isLogin"
            app:argType="boolean" />
    </activity>
</navigation>